{
  "manifest_version": 3,
  "name": "Floyding Twitter",
  "version": "1.0",
  "content_scripts": [
  {
  "js": ["content.js"],
  "matches": ["https://twitter.com/*"],
  "run_at": "document_end"
  }
  ],
  "permissions": [
  "activeTab"
  ],
  "background": {
  "service_worker": "worker.js"
  }
  }